<%= simple_form_for [@user, @tattoo], html: {multipart: true} do |f| %>
  <%= f.file_field :image, label: "Upload an image" %>
  <%= f.input :name, placeholder: "A name/nickname for the tat" %>
  <%= f.input :artist_id, label: "Artist", collection: @artists %>
  <%= f.input :shop_id, label: "Shop" %>
  <%= f.button :submit %>
<% end %>